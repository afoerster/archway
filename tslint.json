{
    "extends": ["tslint-react", "tslint-config-prettier"],
    "rules": {
      "interface-name": false,
      "no-empty-interface": false,
      "ordered-imports": false,
      "interface-over-type-literal": false,
      "indent": [true, "spaces", 2]
    },
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts"
      ]
    }
  }