{
  "name": "heimdali-ui",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build-css": "less-watch-compiler --enable-js --run-once --main-file=index.less src/less src/less",
    "watch-css": "npm run build-css && less-watch-compiler --enable-js --main-file=index.less src/less src/less",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "tsc && webpack --progress",
    "clean": "rd /s/q built",
    "prepare": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "dependencies": {
    "antd": "^3.9.2",
    "chart.js": "^2.7.2",
    "color": "^3.0.0",
    "connected-react-router": "^4.4.1",
    "fetch-intercept": "2.2.3",
    "formik": "^1.2.0",
    "fuse.js": "^3.2.1",
    "history": "latest",
    "node-sass-chokidar": "^1.3.0",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.0",
    "react-syntax-highlighter": "^7.0.0",
    "react-timeago": "^4.1.9",
    "redbox-react": "latest",
    "redux": "^3.7.2",
    "redux-form": "^7.2.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "timeago-react": "^2.0.0",
    "whatwg-fetch": "^2.0.4",
    "yup": "^0.26.5"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/chart.js": "^2.7.32",
    "@types/color": "^3.0.0",
    "@types/history": "^4.7.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-syntax-highlighter": "0.0.6",
    "@types/redux-immutable": "^3.0.38",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.24.9",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-watch-compiler": "^1.11.3",
    "mini-css-extract-plugin": "^0.4.2",
    "open-browser-webpack-plugin": "0.0.5",
    "react-scripts-ts": "^2.17.0",
    "react-test-renderer": "^16.5.1",
    "redux-devtools-extension": "^2.13.5",
    "source-map-loader": "^0.2.4",
    "ts-import-plugin": "^1.5.5",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
